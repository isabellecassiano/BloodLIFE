<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bloodlife">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Localizacao"></activity>
        <activity android:name=".Ranking" />
        <activity android:name=".Conquista" />
        <activity android:name=".Notificacao" />
        <activity android:name=".Perfil_Hemocentro" />
        <activity android:name=".TelaInstrucoes" />
        <activity android:name=".CadastroHemocentro" />
        <activity android:name=".EscolhaCadastro" />
        <activity android:name=".PerfilUsuario" />
        <activity android:name=".TelaCadastroUsuario" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>